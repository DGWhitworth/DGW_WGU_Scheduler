<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dgwwguscheduler">

    <uses-permission android:name="android.permission.SEND_SMS" />



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CourseNotes"
            android:label="@string/title_activity_course_notes"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".DataManagement"
            android:label="@string/title_activity_data_management"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MentorDetail"
            android:label="@string/title_activity_mentor_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MentorList"
            android:label="@string/title_activity_mentor_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AssessmentsDetail"
            android:label="@string/title_activity_assessments_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CoursesDetail"
            android:label="@string/title_activity_courses_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TermsDetail"
            android:label="@string/title_activity_terms_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TermsList"
            android:label="@string/title_activity_terms_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="com.example.dgwwguscheduler.UI.AlertReminder" />
    </application>

</manifest>